{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "FileServConfig": {
    "UseRawName": true,
    "UseUnknowFiles": true,
    "UseDirectoryBrowser": true,
    "FileStore": "./File",
    "LimitSize": "1,000,000,000",
    "ServeMapPath": "/staticFiles"
  },
  "ConnectionStrings": {
    "Default": "Data Source = SpaData.db"
  },
  "AllowedHosts": "*"
}
